
public class Book {

	String name; // 4bytes
	int price; // 4bbytes

	public static void main(String[] args) {

		// classname pyBook = new classname();

		// book1 : reference variable : address = 4353535
		// new Book() : create an object/instance : 8 bytes : 4353535

		Book book1 = new Book();

		Book book2 = new Book();

		book1 = book2;

	}

}
