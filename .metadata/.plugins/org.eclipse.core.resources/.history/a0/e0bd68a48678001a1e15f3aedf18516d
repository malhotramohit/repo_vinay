
public class Calculator {

	public static void main(String[] args) {
		
		//
		int a = 4;
		int b = 6;
		
		// ?
		// swap : a =6, b=4
		
		System.out.println(a);
		System.out.println(b);


		int sum = getSum(4, 5);
		System.out.println(sum);

	}

	public static int getSum(int a, int b) {
		return a + b;
	}

}
