
public class Author {

	String name;
	int age;
	String lang;

	public static void main(String[] args) {

		Author author1 = new Author();
		author1.age = 23;
		author1.name = "Mohit";
		author1.lang = "java";

		Author author2 = new Author();
		author1.age = 24;
		author1.name = "Rohit";
		author1.lang = "c++";

	}

}
